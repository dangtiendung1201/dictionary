<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" styleClass="root" stylesheets="@../Style/Style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DictionaryController">
   <AnchorPane fx:id="container" layoutX="50.0" layoutY="50.0" prefHeight="620.0" prefWidth="1140.0" styleClass="container" stylesheets="@../style/Style.css" />
   <Button fx:id="dictionaryBtn" layoutX="1210.0" layoutY="150.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" styleClass="transparent-button" stylesheets="@../style/Style.css">
      <tooltip>
         <Tooltip fx:id="tooltip1" text="Dictionary" />
      </tooltip>
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../icon/dictionary.png" />
         </ImageView>
      </graphic>
   </Button>
   <Button fx:id="gameBtn" layoutX="1210.0" layoutY="300.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" styleClass="transparent-button" stylesheets="@../style/Style.css">
      <tooltip>
         <Tooltip fx:id="tooltip2" text="Game" />
      </tooltip>
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../icon/game.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button fx:id="APIBtn" layoutX="1209.0" layoutY="450.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" styleClass="transparent-button" stylesheets="@../style/Style.css">
      <tooltip>
         <Tooltip fx:id="tooltip3" text="API" />
      </tooltip>
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../icon/api.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button fx:id="exitBtn" layoutX="1210.0" layoutY="600.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" styleClass="transparent-button" stylesheets="@../style/Style.css">
      <tooltip>
         <Tooltip fx:id="tooltip4" text="Exit" />
      </tooltip>
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../icon/exit.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
</AnchorPane>
